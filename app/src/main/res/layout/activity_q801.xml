                                android:layout_height="wrap_content"
                                android:text="@string/q801d_5" />

                            <RadioButton
                                android:id="@+id/q801d_6"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:text="@string/q801d_6" />

                            <RadioButton
                                android:id="@+id/q801d_7"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:text="@string/q801d_7" />

                            <RadioButton
                                android:id="@+id/q801d_8"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/q801d_8" />

                            <RadioButton
                                android:id="@+id/q801d_10"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:text="@string/q801d_10" />

                            <RadioButton
                                android:id="@+id/q801d_11"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:text="@string/q801d_11" />

                            <RadioButton
                                android:id="@+id/q801d_9"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/q801d_9" />

                            <RadioButton
                                android:id="@+id/q801d_other"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Other Specify" />



                            <View
                                android:layout_width="match_parent"
                                android:layout_marginTop="10dp"
                                android:layout_height="1dp"
                                android:background="@color/gray" />


                            <EditText
                                android:id="@+id/q801d_other1"
                                android:layout_width="244dp"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:background="@drawable/edit_text_background"
                                android:ellipsize="start"
                                android:ems="3"
                                android:gravity="center"
                                android:inputType="text"
                                android:maxLength="2"
                                android:padding="3dp"
                                android:visibility="invisible"/>


                        </RadioGroup>

                    </LinearLayout>

                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">


                    <TextView
                        android:id="@+id/q801etxt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:ems="10"
                        android:padding="3dp"
                        android:textColor="@color/black"
                        android:text="@string/q801e"
                        android:textSize="16sp"
                        />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <RadioGroup
                            android:id="@+id/q801eradioGroup"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginStart="8dp"
                            android:layout_marginLeft="8dp"
                            android:layout_marginTop="30dp"
                            android:layout_marginEnd="8dp"
                            android:layout_marginRight="8dp"
                            android:textSize="16sp"
                            android:visibility="visible">

                            <RadioButton
                                android:id="@+id/q801e_1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:text="@string/q801e_1" />



                            <RadioButton
                                android:id="@+id/q801e_2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:text="@string/q801e_2" />

                            <RadioButton
                                android:id="@+id/q801e_3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:text="@string/q801e_3" />

                            <RadioButton
                                android:id="@+id/q801e_4"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/q801e_4" />

                            <RadioButton
                                android:id="@+id/q801e_5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/q801e_5" />

                            <RadioButton
                                android:id="@+id/q801e_6"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:text="@string/q801e_6" />

                            <RadioButton
                                android:id="@+id/q801e_7"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:text="@string/q801e_7" />

                            <RadioButton
                                android:id="@+id/q801e_8"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/q801e_8" />

                            <RadioButton
                                android:id="@+id/q801e_9"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:text="@string/q801e_9" />


                            <RadioButton
                                android:id="@+id/q801e_other"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Other Specify" />



                            <View
                                android:layout_width="match_parent"
                                android:layout_marginTop="10dp"
                                android:layout_height="1dp"
                                android:background="@color/gray" />


                            <EditText
                                android:id="@+id/q801e_other1"
                                android:layout_width="244dp"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:background="@drawable/edit_text_background"
                                android:ellipsize="start"
                                android:ems="3"
                                android:gravity="center"
                                android:inputType="text"
                                android:maxLength="2"
                                android:padding="3dp"
                                android:visibility="invisible"/>


                        </RadioGroup>

                    </LinearLayout>

                    </LinearLayout>



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">


                    <TextView
                        android:id="@+id/q801ftxt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:ems="10"
                        android:padding="3dp"
                        android:textColor="@color/black"
                        android:text="@string/q801f"
                        android:textSize="16sp"
                        />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <RadioGroup
                            android:id="@+id/q801fradioGroup"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginStart="8dp"
                            android:layout_marginLeft="8dp"
                            android:layout_marginTop="30dp"
                            android:layout_marginEnd="8dp"
                            android:layout_marginRight="8dp"
                            android:textSize="16sp"
                            android:visibility="visible">

                            <RadioButton
                                android:id="@+id/q801f_1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:text="@string/q801f_1" />



                            <RadioButton
                                android:id="@+id/q801f_2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:text="@string/q801f_2" />

                            <RadioButton
                                android:id="@+id/q801f_3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:text="@string/q801f_3" />

                            <RadioButton
                                android:id="@+id/q801f_4"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/q801f_4" />

                            <RadioButton
                                android:id="@+id/q801f_9"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/q801f_9" />



                        </RadioGroup>

                    </LinearLayout>

                </LinearLayout>



























