                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:ems="10"
                        android:padding="3dp"
                        android:textColor="@color/black"
                        android:text="@string/q801c"
                        android:textSize="16sp"
                        />

                    <LinearLayout
